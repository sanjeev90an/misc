
## location - /etc/NetworkManager/system-connections/firefly

[connection]
id=firefly
uuid=e13b506b-62ec-4501-966d-ab9f32061d15
type=802-11-wireless
autoconnect=true

[802-11-wireless]
ssid=firefly
mode=ap
mac-address=34:E6:AD:8C:F2:EE
security=802-11-wireless-security

[802-11-wireless-security]
key-mgmt=wpa-psk
proto=rsn;
pairwise=ccmp;
group=ccmp;
psk=pass

[ipv4]
method=shared

[ipv6]
method=auto
